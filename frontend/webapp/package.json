{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "check-peer-dependencies --runOnlyOnRootDependencies",
    "dev": "next dev --turbopack",
    "prebuild": "rm -rf out .next",
    "build": "cross-env NODE_ENV=production next build --turbopack",
    "back:build": "cd .. && go build -o odigos-backend",
    "back:start": "cd .. && ./odigos-backend --port 8085 --debug --address 0.0.0.0",
    "cy:run": "cypress run --e2e -q",
    "cy:open": "cypress open --e2e -b electron",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@apollo/experimental-nextjs-app-support": "^0.12.3",
    "@odigos/ui-kit": "^0.0.198",
    "graphql": "^16.12.0",
    "next": "15.5.12",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "^6.1.1",
    "styled-components": "6.2.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@types/node": "25.3.1",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.27",
    "check-peer-dependencies": "^4.3.4",
    "cross-env": "^10.1.0",
    "cypress": "^15.10.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "typescript": "5.9.3"
  }
}
